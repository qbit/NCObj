  br
  img( src="/images/loading.gif" )

div( id='overlay' )

div( id='container' )
  div( id='inp' )
    form( id='form', method='post' )
      p
        label X:
          input( name='x', value='0', type='text' )
      p
        label Y:
          input( name='y', value='0', type='text' )
